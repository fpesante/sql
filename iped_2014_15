SELECT HD2014.STABBR, HD2014.INSTNM, EF2014CP.EFCIPLEV, C2014_A.CIPCODE, valuesets14.valueLabel
FROM ((HD2014 INNER JOIN EF2014CP ON HD2014.UNITID = EF2014CP.UNITID) 
    INNER JOIN C2014_A ON HD2014.UNITID = C2014_A.UNITID) 
    INNER JOIN valuesets14 ON EF2014CP.CIPCODE = valuesets14.Codevalue
WHERE (((HD2014.STABBR)="PR") AND ((EF2014CP.EFCIPLEV)=302) AND ((C2014_A.CIPCODE) Like "**.***")) 
    OR (((HD2014.STABBR)="PR") AND ((EF2014CP.EFCIPLEV)=312)) 
    OR (((HD2014.STABBR)="PR") AND ((EF2014CP.EFCIPLEV)=816)) 
    OR (((HD2014.STABBR)="PR") AND ((EF2014CP.EFCIPLEV)=916));
